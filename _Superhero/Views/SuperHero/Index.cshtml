@model IEnumerable<_Superhero.Models.People>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Age)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.alterEgo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.primeryAbility)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.secondaryAbility)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.catchphrase)
        </th>
        <th></th>
    </tr>
   
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Age)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.alterEgo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.primeryAbility)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.secondaryAbility)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.catchphrase)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
